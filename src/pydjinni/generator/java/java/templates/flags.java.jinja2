{% extends "base.jinja2" %}

{% block content %}
{% if type_def.comment %}
{{ type_def.java.comment }}
{% endif %}
public enum {{ type_def.java.name }} {
{% for flag in type_def.flags if not flag.none and not flag.all %}
    {% if flag.comment %}
    {{ flag.java.comment | indent(4) }}
    {% endif %}
    {{ flag.java.name ~ (";" if loop.last else ",") }}
{% endfor %}
}
{% endblock %}
